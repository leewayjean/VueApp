{
  "name": "vue-ele-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "@better-scroll/core": "^2.0.0-beta.2",
    "axios": "^0.19.0",
    "better-scroll": "^2.0.0-beta.2",
    "core-js": "^3.3.2",
    "vant": "^2.2.14",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-service": "^5.0.8",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.12.2",
    "postcss-px-to-viewport": "^1.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "unitToConvert": "px",
        "viewportWidth": 320,
        "unitPrecision": 5,
        "viewportUnit": "vw",
        "fontViewportUnit": "vw",
        "selectorBlackList": [],
        "minPixelValue": 1,
        "mediaQuery": false
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
